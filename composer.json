{
  "name": "barthy/image-upload-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides commonly used functions to upload and handle images in symfony applications. It primarily is meant to work with barthy/admin-bundle.",
  "keywords": [
    "backend",
    "admin",
    "generator"
  ],
  "homepage": "https://github.com/BarthyB/image-upload-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Barthy",
      "homepage": "https://github.com/BarthyB",
      "email": "post@barthy.koeln"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "a2lix/translation-form-bundle": "^3.0",
    "barthy/cached-prezent-translation": "^1.0",
    "cocur/slugify": "^3.2",
    "liip/imagine-bundle": "^2.1",
    "prezent/doctrine-translatable-bundle": "^1.0",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/form": "^4.1",
    "symfony/orm-pack": "^1.0",
    "symfony/validator": "^4.1",
    "twig/twig": "^2.11 || ^3.0",
    "vich/uploader-bundle": "^1.8"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/barthy-koeln/cached-prezent-translation.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Barthy\\ImageUploadBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Barthy\\ImageUploadBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "dama/doctrine-test-bundle": "^6.2",
    "phpunit/phpunit": "^7.5",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "^4.1",
    "symfony/console": "^4.1",
    "symfony/expression-language": "^4.1",
    "symfony/flex": "^1.0",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "^4.1",
    "symfony/translation": "^4.1",
    "symfony/twig-bundle": "^4.1",
    "symfony/web-link": "^4.1",
    "symfony/yaml": "^4.1"
  }
}
