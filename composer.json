{
  "name": "barthy/image-upload-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides commonly used functions to upload and handle images in symfony applications. It primarily is meant to work with barthy/admin-bundle.",
  "keywords": [
    "backend",
    "admin",
    "generator"
  ],
  "homepage": "https://github.com/BarthyB/image-upload-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Barthy",
      "homepage": "https://github.com/BarthyB",
      "email": "post@barthy.koeln"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "a2lix/translation-form-bundle": "^3.0",
    "barthy-koeln/cached-prezent-translation": "^1.0",
    "cocur/slugify": "^3.2 || ^4.0",
    "liip/imagine-bundle": "^2.1",
    "prezent/doctrine-translatable-bundle": "^1.0",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/form": "^4.4|^5.0",
    "symfony/validator": "^4.4 || ^5.0",
    "twig/twig": "^2.11 || ^3.0",
    "vich/uploader-bundle": "^1.8"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BarthyKoeln\\ImageUploadBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "hooks": {
      "pre-commit": [
        "bin/pre-commit"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "brainmaestro/composer-git-hooks": "^2.8",
    "codeception/assert-throws": "^1.1",
    "dama/doctrine-test-bundle": "^6.2",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/orm": "^2.7",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpmd/phpmd": "^2.7",
    "phpunit/phpunit": "^9.4",
    "sensio/framework-extra-bundle": "^5.1",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/asset": "^5.1",
    "symfony/console": "^5.1",
    "symfony/expression-language": "^5.1",
    "symfony/flex": "^1.0",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "^5.1",
    "symfony/translation": "^5.1",
    "symfony/twig-bundle": "^5.1",
    "symfony/web-link": "^5.1",
    "symfony/yaml": "^5.1"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
